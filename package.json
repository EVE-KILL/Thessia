    "type": "module",
    "scripts": {
        "build": "nuxt build && bun run build.ts all",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "debug": "nuxt dev --inspect --host 0.0.0.0",
        "build:cli": "bun run build.ts imports && bun run build.ts cli",
        "build:cron": "bun run build.ts imports && bun run build.ts cron",
        "build:queue": "bun run build.ts imports && bun run build.ts queue",
        "build:loaders": "bun run build.ts loaders",
